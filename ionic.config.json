{
  "name": "awesome-todolist-mobile",
  "integrations": {},
  "type": "ionic1",
  "watchPatterns": ["scss/**/*", "www/**/*", "!www/lib/**/*", "!www/**/*.map"],

  "proxies": [
    {
      "path": "/tasks",
      "proxyUrl": "https://todo-list-p.herokuapp.com/api/tasks"
    },
    {
      "path": "/lists",
      "proxyUrl": "https://todo-list-p.herokuapp.com/api/lists"
    },
    {
      "path": "/auth",
      "proxyUrl": "https://todo-list-p.herokuapp.comapi/auth"
    },
    {
      "path": "/logout",
      "proxyUrl": "https://todo-list-p.herokuapp.com/logout"
    },
    {
      "path": "/register",
      "proxyUrl": "https://todo-list-p.herokuapp.com/api/users"
    }
  ]
}
